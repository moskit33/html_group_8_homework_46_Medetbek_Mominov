doctype html
html(lang="en")
	head
		meta(charset="UTF-8")
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		link(href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900", rel="stylesheet")
		link(rel="stylesheet", href="css/reset.css")
		link(rel="stylesheet", href="css/style.css")
		title= title
	body
		header.header__main
		header.header__
		.container
			.main
				.sidebar
					block sidebar
						nav.menu
							h5.menu__title Products menu
							each item in menu__items
								a(href="#").menu__item= item
				main.content
					block content